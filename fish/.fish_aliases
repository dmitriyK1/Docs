alias up ~/update.sh

alias g "git"
alias grepall="git branch -a | tr -d \* | sed '/->/d' | xargs git grep"
alias gdl "git difftool | less"
alias gdlc "git difftool --cached | less"
alias gf "git fetch"
alias gs "git status "
alias gsy "git sync "
alias gb "git branch "
alias gbs "git branches"
alias gc "git commit -m "
alias gco "git checkout "
alias gch "git checkout "
alias gd "git diff "
alias gdc "git diff --cached "
alias ga "git add "
alias gl "git log "
alias gr "git rebase -i "
alias gps "git push -u "
alias gpl "git pull "
alias gsh "git show "
alias gshn "git show --name-only"
alias grv "git remote -vv"
alias gsw "git switch "
alias gcp "git cherry-pick "
alias gcf "git commit --no-verify -m "
alias gpf "git push --no-verify "
alias gpu "git publish"
# alias h "how2 -l javascript "

alias vim "/usr/local/bin/vim"
alias v "vim "
alias bi "brew install "
alias bu "brew uninstall "
alias bs "brew search "
alias p "percol "
alias ss "source ~/.config/fish/config.fish"
alias vless='/usr/share/vim/vim74/macros/less.sh '

alias cp='cp -iv'                           # Preferred 'cp' implementation
alias mv='mv -iv'                           # Preferred 'mv' implementation
alias mkdir='mkdir -pv'                     # Preferred 'mkdir' implementation

alias numFiles='echo (ls -1 | wc -l)'       # numFiles:     Count of non-hidden files in current dir

alias emacs "/Applications/Emacs.app/Contents/MacOS/Emacs -nw"
alias em='emacs'
alias cat=ccat

alias fp='ls | fpp'

alias cal='cal | grep --color -EC6 "\b"(date +%e | sed "s/ //g")'

alias rg='rg -i --color=always'
